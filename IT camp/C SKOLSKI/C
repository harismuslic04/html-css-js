     b++;
     if(k==v){
         f++;
         z=b;
     }
     if(k<v){
         m++;
     }
 }
 if(f>0){
     printf("broj se nalazi u nizu na %d. mestu,ima %d brojeva manjih od njega",z,m);
 }
 else if(f==0){
     printf("broj se ne nalazi u nizu");
 }
