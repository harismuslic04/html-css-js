 printf("%c\n%2c\n%3c\n%4c\n%5c", 'A', 'B', 'C', 'D', 'E');
}
int main()
{
 char x, y, z;
 printf("Unesite tri znaka:\n");
 scanf("%c %c %c",&x,&y,&z);
 printf("Promenljiva\t ASCII kod\n");
 printf("%c\t\t %d\n",x, x);
 printf("%c\t\t %d\n",y, y);
 printf("%c\t\t %d\n",z, z);
 return 0;
}
int main()
{
printf("\nTip CHAR = %d B memorije.", sizeof(char));
 printf("\nTip UNSIGNED CHAR = %d B memorije.", sizeof(unsigned char));
 printf("\nTip INT = %d B memorije.", sizeof(int));
 printf("\nTip UNSIGNED INT = %d B memorije.", sizeof(unsigned int));
 printf("\nTip LONG = %d B memorije.", sizeof(long));
 printf("\nTip UNSIGNED LONG = %d B memorije.", sizeof(unsigned long));
 printf("\nTip FLOAT = %d B memorije.", sizeof(float));
 printf("\nTip DOUBLE = %d B memorije.", sizeof(double));

 return 0; 

}
int main()
{
int a,b,pomocna;
printf("Unesite prvi broj a= ");
scanf("%d",&a);
printf("Unesite drugi broj b=");
scanf("%d",&b);
printf("Pre zamene: \ta=%d ,b=%d",a,b);
pomocna=a;
a=b;
b=pomocna;
printf("\nPosle zamene:\ta=%d,b=%d\n\n",a,b);
return 0;
}
int main()
{
int a,b,c;
printf("Unesite prvi broj:");
scanf("%d",&a);
printf("Unesite drugi broj:");
scanf("%d",&b);
c = a + b;
printf("Zbir unetih brojeva je: %d\n",c);
return 0;
}
int main()
{
int a,b,c;
printf("Unesite prvi broj:");
scanf("%d",&a);
printf("Unesite drugi broj:");
scanf("%d",&b);
printf("Zbir unetih brojeva je: %d\n",a+b);
printf("Proizvod unetih brojeva je: %d\n",a*b);
printf("Razlika unetih brojeva je: %d\n",a-b);
printf("Kolicnik unetih brojeva je: %d\n",a/b);
printf("Realni kolicnik unetih brojeva je:%f\n",(float)a/(float)b);
printf("Ostatak pri deljenju uneta dva broja je:%d\n",a%b);
return 0;
}
int main()
{
 float a,b,o,p;
 printf("Unesite stranice pravougaonika:");
 scanf("%f %f",&a,&b);
 o=2*a+2*b;
 printf("\nObim pravougaonika je: %.1f",o);
  p=a*b;
 printf("\nPovrsina pravougaonika je: %.1f",p);

}
#define PI 3.14
int main()
{
 float r,p;
 printf("Unesite poluprecnik kruga:");
 scanf("%f",&r);
 printf("\nPovrsina kruga je: %.2f",r*r*PI);

}
#define PI 3.14
int main()
{
    float r,h,p,o;
 printf("Unesite poluprecnik osnovice valjka:");
 scanf("%f",&r);
 printf("Unesite visinu valjka:");
 scanf("%f",&h);
 p=2*r*PI*(r+h);
 printf("Povrsina valjka je: %.4f\n",p);
 return 0;
}
{
  float a,b,x;
  printf("Unesi a: ");
  scanf("%f",&a);
  printf("Unesi b: ");
  scanf("%f",&b); 
  x=-b/a;
  printf("Jednacina ima oblik %.2fx + %.2f=0\n",a,b);
  printf("Resenje jednacine je x=%.2f\n",x);
  return 0;
}
#include <math.h>
int main()
{
  float x,y;
  printf("Unesite pozitivnu vrednost za x: \n");
  scanf("%f",&x);
  y=sqrt(x+x*x+x*x*x);
  printf("y=%.2f",y);
  return 0;
}
{
float a1,b1,a2,b2,z1,z2,rz,iz,rz1,iz1;
printf("Unesi a1:");
scanf("%f",&a1);
printf("Unesi b1:");
scanf("%f",&b1);
printf("Unesi a2:");
scanf("%f",&a2);
printf("Unesi b2:");
scanf("%f",&b2);
z1=a1+b1;
z2=a2+b2;
printf("z1=%.1f+%.1fi",a1,b1);
printf("\nz2=%.1f+%.1fi",a2,b2);
rz=a1+a2;
iz=b1+b2;
printf("\nz1+z2=%.1f+%.1fi",rz,iz);
rz1=a1-a2;
iz1=b1-b2;
printf("\nz1-z2=%.1f+%.1fi",rz1,iz1);
return 0;
}
double x1, x2, y1, y2, z1, z2, d;
 printf("Unesite koordinate prve tacke <x1,y1,z1>: \n");
 scanf("%lf %lf %lf",&x1,&y1,&z1);
 printf("Unesite koordinate druge tacke <x2,y2,z2>: \n");
 scanf("%lf %lf %lf",&x2,&y2,&z2);
 d=sqrt(pow(x2-x1,2)+pow(y2-y1,2)+pow(z2-z1,2));
 printf("\nRastojanje d = %.2f\n", d);
 getche();
 return 0; 

 double xA, yA, xB, yB, xC, yC, a, b, c, s, P;
 printf("Koordinate temena trougla\n");
 printf("- prvo teme <xA,yA>: "); scanf("%lf%lf",&xA,&yA);
 printf("- drugo teme <xB,yB>: "); scanf("%lf%lf",&xB,&yB);
 printf("- trece teme <xC,yC>: "); scanf("%lf%lf",&xC,&yC);
 a=sqrt(pow(xB-xC,2)+pow(yB-yC,2));
 b=sqrt(pow(xC-xA,2)+pow(yC-yA,2));
 c=sqrt(pow(xA-xB,2)+pow(yA-yB,2));
 s=(a+b+c)/2;
 P=sqrt(s*(s-a)*(s-b)*(s-c));
 printf("\nPovrsina trougla: %.2f\n", P);
 getche();
 return 0; 

{
int xyz,x,z,y;
printf("Unesite trocifren broj:");
scanf("%d",&xyz);
x=xyz/100;
y=(xyz/10)%10;
z=xyz%10;
printf("Cifre unetog broja %d su %d, %d, %d\n",xyz,x,y,z);
printf("\nSuma cifara je: %d", x+y+z); 
return 0;
}
int a,b,c,p;
printf("Unesite vrednost proizvoda u dinarima:");
scanf("%d",&p);
a=p/500;
printf("Potreban broj novcanina u iznosu od 500 dinara je %d",a);
b=(p-(a*500))/100;
printf("\nPotreban broj novcanina u iznosu od 100 dinara je %d",b);
c=(p-(a*500+b*100))/10;
printf("\nPotreban broj novcanina u iznosu od 10 dinara je %d",c);
return 0;

{
long sec, d, h, m, s;
 printf("Unesite vreme u sekundama: ");
 scanf("%ld",&sec);
 d=sec/86400;
 h=(sec%86400)/3600;
 m=(sec%(d*86400+h*3600))/60;
 s=(sec%(d*86400+h*3600+m*60));
 printf("\n(d:h:m:s) %ld,%ld,%ld,%ld:",d,h,m,s);
 return 0; 
}
{
 double x;
 int stepen,minut,sekund;
 printf("Unesite ugao u radijanima: ");
 scanf("%lf",&x);
 x*=180/PI; /*konverzija u stepene*/
 stepen=(int)x; /*zaokruzivanje broja stepeni*/
 x-=stepen; /*izracunavanje koliko delova stepena je preostalo*/
 x*=60; /*izracunavanje broja minuta*/
 minut=(int)x; /*zaokruzivanje broja minuta*/
 x-=minut; /*koliko delova minuta je ostalo*/
 x*=60; /*izracunavanje broja sekundi*/
 sekund=(int)x; /*zaokruzivanje broja sekundi*/
 printf("\nstepena, minuta, sekundi [%d,%d,%d]\n", stepen, minut, sekund);
 return 0; 
}

 int x, y;
 int a=0, b=0;
 printf("Na pocetku : \na = %d\nb = %d\n", a, b);
 a++;
 ++b;
 printf("\nPosle : a++; ++b; \na = %d\nb = %d\n", a, b);
 x = ++a;
 y = b++;
 printf("\nPosle : x = ++a; \na = %d\nx = %d\n", a, x);
 printf("\nPosle : y = b++; \nb = %d\ny = %d\n", b, y);
 return 0;

{
int x=1, y=2, z=3;
 printf("Vrednost izraza x==1 je logicka %d.\n", x==1);
 printf("Vrednost izraza y!=2 je logicka %d.\n", y!=2);
 printf("Vrednost izraza x<=y je logicka %d.\n", x<=1);
 printf("Vrednost izraza x>=z je logicka %d.\n", x>=1);
 printf("Vrednost izraza x+y==z je logicka %d.\n", x+y==z);
 printf("Vrednost izraza x>y*z je logicka %d.\n", x>y*z);
 return 0; 
}
{
int a = 5<3, b = 5>3, c = 3==5, d = 3!=5;
 printf("5<3 - %d\n5>3 - %d\n3==5 - %d\n3!=5 - %d\n", a, b, c, d);
 printf("Konjunkcija : 3>5 && 5>3 - %d\n", a && b);
 printf("Disjunkcija : 3>5 || 5>3 - %d\n", a || b);
 printf("Negacija : !(3>5) - %d\n", !a);
 return 0; 
}
//!!!!!!!!!!!!!!!OVAJ MI NIJE JASAN NI SEKUNDE SEKUNDARNE!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
 int x, y, n;      
 printf("Unesite dva heksadecimalna broja: ");
 scanf("%i %i", &x, &y);
 printf("Unesite broj pomeraja: ");
 scanf("%d", &n);
 printf("%#x & %#x = %#x\n", x, y, x&y);
 printf("%#x | %#x = %#x\n", x, y, x|y);
 printf("%#x ^ %#x = %#x\n", x, y, x^y);
 printf("%#x << %d = %#x\n", x, n, x<<n);
 printf("%#x >> %d = %#x\n", y, n, y>>n);
 getche();
 return 0; 

{
 int broj;
 printf("Unesite broj: ");
 scanf("%d", &broj);
 if (broj%2 == 0)
 printf("\nUneti broj je paran.\n");
 else
 printf("\nUneti broj je neparan.\n");
 return 0; 
}
{
 int broj;
 printf("Unesite broj:");
 scanf("%d",&broj);
 if(broj>0)
 printf("\nBroj je pozitivan");
 else if(broj<0)
 printf("\nUneti broj je negativan.\n");
  else
 printf("\nUneti broj je jednak 0.\n");
 return 0;
}
{
 int a, b;
 printf("Unesite broj a= ");
 scanf("%d", &a);
 printf("Unesite broj b= ");
 scanf("%d", &b);
 if(a == b)
 printf("Brojevi su jednaki.\n");
 else if (a > b)
 printf("Broj a je veci od b.\n");
 else
 printf("Broj a je manji od b.\n");
 return 0;
}
int a,b,c;
printf("Unesite broj a:");
scanf("%d",&a);
printf("Unesite broj b:");
scanf("%d",&b);
printf("Unesite broj c:");
scanf("%d",&c);
if(a>b && a>c)
printf("Broj a je najveci");
else if(b>a && b>c)
printf("Broj b je najveci");
else
printf("Broj c je najveci");
return 0;
}
double x, y, z, p;    //NE KONTAAAAAAAAAAAAAAM!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!1
 printf("Unesite tri realna broja: ");
 scanf("%lf%lf%lf", &x, &y, &z);
 if(x>y)
 {
 p=x; x=y; y=p;
 }
 if(x>z)
 {
 p=x; x=z; z=p;
 }
 if(y>z)
 {
 p=y; y=z; z=p;
 }
 printf("Uredjeni brojevi: %.2f %.2f %.2f\n", x, y, z);
 return 0; 
}
int n,k;
 printf(" Unesite broj: ");
 scanf("%d",&n);
 printf(" Unesite poziciju tog broja koju zelite da proverite: ");
 scanf("%d",&n);
 if((n & (1 << k))!=0)
 printf(" Bit je 1\n");
 else
 printf(" Bit je 0\n");
 return 0;

 int a;
 printf("Unesite broj bodova: ");
 scanf ("%d", &a);
 if (a>90)
 printf("Ocena je 10\n");
 else if (a>80)
 printf("Ocena je 9\n");
 else if (a>70)
 printf("Ocena je 8\n");
 else if (a>60)
 printf ("Ocena je 7\n");
 else if (a>50)
 printf("Ocena je 6\n");
 else
 printf("Ocena je 5\n");
 return 0; 

 {
 int a, b, x;
 printf("Unesite donju i gornju granicu opsega: ");
 scanf("%d %d", &a, &b);
 printf("Unesite ceo broj x= ");
 scanf("%d", &x);
 if((a <= x*x) && (x*x <= b))
 printf("\nx*x=%d pripada oblasti [%d,%d]", x*x, a, b);
 else
 printf("\nx*x=%d ne pripada oblasti [%d,%d]", x*x, a, b);
 return 0; 
}
{
  int x, y;
 printf("Unesite dva cela broja: ");
 scanf("%d %d", &x, &y);
 if(x<y)
 printf("\nRezultat = %d", y-x);
 else
 printf("\nRezultat = %d", x-y);
return 0;
}
 float x;
 int y;
 printf("Unesite x= ");
 scanf("%f", &x);
 if(x==0)
 y=0;
 else if(x>0)
 y=1;
 else
 y=-1;
 printf("\ny= %d", y);
 return 0;

 {
 float x, y;
 printf ("Unesite x= "); scanf("%f", &x);
 if(-2<x<=2)
 y=2*x;
 else if(5<=x<7)
 y=3*x-1;
 else
 y=1/x;
 printf("\ny= %.2f", y);
 return 0;
}
double xa, ya, xb, yb, xc, yc, a, b, c, alfa, beta, gama;
 printf("Unesite koordinate tacke A(x,y): ");
 scanf("%lf%lf",&xa,&ya);
 printf("Unesite koordinate tacke B(x,y): ");
 scanf("%lf%lf",&xb,&yb);
 printf("Unesite koordinate tacke C(x,y): ");
 scanf("%lf%lf",&xc,&yc);
 a = sqrt(pow(xb-xc,2) + pow(yb-yc,2));
 b = sqrt(pow(xa-xc,2) + pow(ya-yc,2));
 c = sqrt(pow(xa-xb,2) + pow(ya-yb,2));
 printf("\nStranice:\n");
 printf("a= %f\n", a);
 printf("b= %f\n", b);
 printf("c= %f\n", c);
 if(a+b>c && a+c>b && b+c>a)
 {
 alfa = acos( (b*b+c*c-a*a) / (2*b*c) );
 beta = acos( (c*c+a*a-b*b) / (2*c*a) );
 gama = acos( (a*a+b*b-c*c) / (2*a*b) );
 alfa *= 180/PI;
 beta *= 180/PI;
 gama *= 180/PI;
 printf("\nUglovi:\n");
 printf("Alfa = %.2f stepeni\n", alfa);
 printf("Beta = %.2f stepeni\n", beta);
 printf("Gama = %.2f stepeni\n", gama);
 }
 else 
 printf("Ne moze se kreirati trougao.\n");
 return 0; 

{
double a1, a2, b1, b2, x, y;
 printf("Unesite koeficijente:\na1= "); scanf("%lf", &a1);
 printf("b1= "); scanf("%lf", &b1);
 printf("a2= "); scanf("%lf", &a2);
 printf("b2= "); scanf("%lf", &b2);
 printf("\nPrave:\ny=%.2fx+%.2f\ny=%.2fx+%.2f\n", a1, b1, a2, b2);
 if(a1==a2)
 {
 if(b1==b2) printf("\nPrave su podudarne.");
 else printf("\nPrave su paralelne.");
 }
 else
 {
 x=(b2-b1)/(a1-a2);
 y=a1*x+b1;
 printf("\nTacka preseka je x= %.2f, y= %.2f\n", x, y);
 }
 return 0; 
}
int main()
{
float n,s,i;
printf("Unesite broj;");
scanf("%f",&n);
s=0;
for(i=1;i<=n;i++)
s+=i;
printf("Suma je %.0f",s);
return 0;
}
 int n, i;
 float a, s, ars;
 s=0;
 printf("n= ");
 scanf("%d",&n);
 printf(" Unesite %d brojeva:\n", n);
 for(i=1; i<=n; i++)
 {
 scanf("%f",&a);
 s+=a;
 }
{
 int i,n;
 long faktorijel=1;
 printf("n= ");
 scanf("%d",&n);
 for(i=1;i<=n;i++)
 faktorijel*=i;
 printf("Faktorijel = %ld\n", faktorijel);
 return 0; 
} ars=s/n;
 printf("\n Suma zadatih brojeva je %.2f", s);
 printf("\n Aritmeticka sredina zadatih brojeva je %.2f", ars);
 getche();
 return 0; 

{
 int i,n;
 long f=1,s=0;
printf("Unesite broj n:");
scanf("%d",&n);
for(i=1;i<=n;i++)
{
    f*=i;
    s+=f;
}
printf("suma = %ld \n",s);
return 0;
}
{
 int i,n;
 long f=1,s=0;
printf("Unesite broj n:");
scanf("%d",&n);
for(i=1;i<=n;i++)
{
    f*=i;
    s+=f;
}
printf("suma = %ld \n",s);
return 0;
}
 int i,n;
 long f=1, s=0;
 printf("n= ");
 scanf("%d",&n);
 for(i=1; i<=n; i++)
 {
 f*=i;
 s+=f;
 }
 printf("\ns= %ld\n", s);
 getche();
 return 0;

  int i, m, n;
 long s;
 printf("n= ");
 scanf("%d",&n);
 printf("m= ");
 scanf("%d",&m);
 s=1;
 for(i=0;i<=m;i++)
 s=s*(n+i*m);
 printf("\ns= %ld\n", s);
 getche();
 return 0; 

{
 int i, m, n, znak;
 float s;
 printf("n= ");
 scanf("%d",&n);
 printf("m= ");
 scanf("%d",&m);
 s=0;
 znak=1;
 for(i=1;i<=m;i++)
 {
 s=s+(float)znak/(n+i*m);
 znak=-znak;
 }
 printf("\ns= %.5f\n", s);
 return 0; 
}

int i, n, fakt;
 float s, q;
 printf("n= ");
 scanf("%d",&n);
 s=0;
 q=0;
 fakt=1;
 for(i=1; i<=n; i++)
 {
 fakt=fakt*i;
 q=q+1./(1+i);
 s=s+fakt/q;
 }
 printf("\ns= %.5f\n", s);
 return 0; 