// januar 2020 ispit
int suma,i,n,brojac=0,suma2=0,broj;
printf("unesi sumu");
scanf("%d",&suma);
while(1){
    printf("unesi broj:");
    scanf("%d",&n);
    broj=n;
    for(i=1;i<=n;i++){
        if(n%i==0)
            brojac++;
        
    }
    if(brojac==2){
        suma2+=broj;
    }
    if(suma2>suma){
        printf("suma je %d",suma2);
        break;
    }
    brojac=0;
}
return 0;
}
