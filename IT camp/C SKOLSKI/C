 scanf ("%f", &op1);
 printf(" Operand2: ");
 scanf ("%f", &op2);
 switch(op)
 {
 case '+':
 printf("Vrednost: %.2f\n", op1 + op2);
 break;
 case '-':
 printf("Vrednost: %.2f\n", op1 - op2);
 break;
 case '*':
 printf("vrednost: %.2f\n", op1 * op2);
 break;
 case '/':
 if (op2 == 0)
 printf("Greska! Deljenje nulom!\n");
 else
 printf("\n Rezultat: %.2f\n", op1 / op2);
 break;
 default:
 printf(" Greska! Pogresan operator\n");
 }
 return 0;  
