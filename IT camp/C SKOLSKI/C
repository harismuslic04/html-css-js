// I kolokvijum 2019
{
int a,b,i,a1,b1,cifraa,cifrab,k=-1,n=-1,suma1=0,suma2=0;
float suma=0;
printf("unesi a:");
scanf("%d",&a);
printf("unesi b:");
scanf("%d",&b);
while(a>0){
    cifraa=a%10;
    a/=10;
    k++;
    suma1+=cifraa*pow(2,k);
}
while(b>0){
    cifrab=b%10;
    b/=10;
    n++;
    suma2+=cifrab*pow(2,n);
}
suma=suma1+suma2;
printf("zbir %d i %d je %f",a,b,suma);
return 0;
}
