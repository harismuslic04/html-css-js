  float nizA[MAX], nizB[MAX], nizC[MAX];
 int i;
 printf (" Elementi niza A: ");
 for(i=0; i<MAX; i++)
 scanf ("%f", &nizA[i]);
 printf (" Elementi niza B: ");
 for(i=0; i<MAX; i++)
 scanf ("%f", &nizB[i]);
 printf ("\n Elementi niza C: ");
 for(i=0; i<MAX; i++)
 {
 nizC[i]=pow(nizA[i],3)/3+2*nizA[i]*nizB[i];
 printf ("%.2f ", nizC[i]);
 }
