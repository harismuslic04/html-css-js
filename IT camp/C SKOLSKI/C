{
 float xmin, xmax, dx, x, y;
 printf("xmin= ");
 scanf("%f", &xmin);
 printf("xmax= ");
 scanf("%f", &xmax);
 printf("dx= ");
 scanf("%f", &dx);
 printf("\n x y\n ===================\n");
 for(x=xmin; x<=xmax; x+=dx)
 {
 y=(2*x+1)/(x*x-1);
 printf ("\n%f", y);
 }
 return 0; 
}
{
double xmin, xmax, dx, x, y=1, p=1;
 int i, n;
 printf("n= ");
 scanf("%d", &n);
 printf("xmin= ");
 scanf("%lf", &xmin);
 printf("xmax= ");
 scanf("%lf", &xmax);
 printf("dx= ");
 scanf("%lf", &dx);
 printf("\n x y\n");
 printf("===================\n");
 for(x=xmin;x<=xmax;x+=dx)
 {
 for(i=1; i<=n; i++)
 {
 p*=x;
 y*=(1+p);
 }
 printf("%10.6f %11.6f\n",x,y);
 p=1;
 y=1;
 }
 return 0; 
}
#include <stdio.h>
#include <math.h>
#define PI 3.141592 
int main()
{
 double dx, UgaoRad, UgaoSte;
 printf("Unesite korak u stepenima: ");
 scanf("%lf",&dx);
 printf("\n x sin(x)");
 printf("\n=======================\n");
 for(UgaoSte=0; UgaoSte<=90; UgaoSte+=dx)
 {
 UgaoRad = (PI*UgaoSte)/180.0;
 printf("%f \t%f\n",UgaoSte, sin(UgaoRad));
 }
 return 0;  
}
