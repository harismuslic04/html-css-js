{
 int i, m, n, znak;
 float s;
 printf("n= ");
 scanf("%d",&n);
 printf("m= ");
 scanf("%d",&m);
 s=0;
 znak=1;
 for(i=1;i<=m;i++)
 {
 s=s+(float)znak/(n+i*m);
 znak=-znak;
 }
 printf("\ns= %.5f\n", s);
 return 0; 
}

int i, n, fakt;
 float s, q;
 printf("n= ");
 scanf("%d",&n);
 s=0;
 q=0;
 fakt=1;
 for(i=1; i<=n; i++)
 {
 fakt=fakt*i;
 q=q+1./(1+i);
 s=s+fakt/q;
 }
 printf("\ns= %.5f\n", s);
 return 0; 

 {
     double a, b, c, D, x1, x2;
 printf("Unesite koeficijente:\na= "); scanf("%lf",&a);
 printf("b= "); scanf("%lf",&b);
 printf("c= "); scanf("%lf",&c);
 if(a!=0)
 {
 D=b*b-4*a*c;
 if(D>0)
 {
 x1=(-b+sqrt(D))/(2*a);
 x2=(-b-sqrt(D))/(2*a); 
printf("\nResenja:\nx1=%.2f, x2=%.2f",x1,x2);
 }
 else if(D==0)
 {
 x1=(-b/(2*a));
 printf("\nResenje:\nx1=x2=%.2f",x1);
 }
 else
 {
 x1=-b/(2*a);
 x2=sqrt(-D)/(2*a);
 printf("\nKompleksna resenja:\n");
 printf("x1=%.2f+i%.2f, x2=%.2f-i%.2fi",x1,x2,x1,x2);
 }
 }
 else
 {
 if(b!=0)
 {
 x1=-c/b;
 printf("\nResenje:\nx=%.2f",x1);
 }
 else printf("Sistem nema resenja.");
 }
 return 0; 
}
