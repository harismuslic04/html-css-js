 scanf("%d", &a);
 printf("Unesite broj b= ");
 scanf("%d", &b);
 if(a == b)
 printf("Brojevi su jednaki.\n");
 else if (a > b)
 printf("Broj a je veci od b.\n");
 else
 printf("Broj a je manji od b.\n");
 return 0;
}
int a,b,c;
printf("Unesite broj a:");
scanf("%d",&a);
printf("Unesite broj b:");
scanf("%d",&b);
printf("Unesite broj c:");
scanf("%d",&c);
if(a>b && a>c)
printf("Broj a je najveci");
else if(b>a && b>c)
printf("Broj b je najveci");
else
printf("Broj c je najveci");
return 0;
}
double x, y, z, p;    //NE KONTAAAAAAAAAAAAAAM!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!1
 printf("Unesite tri realna broja: ");
 scanf("%lf%lf%lf", &x, &y, &z);
 if(x>y)
 {
 p=x; x=y; y=p;
 }
 if(x>z)
 {
 p=x; x=z; z=p;
 }
 if(y>z)
 {
 p=y; y=z; z=p;
 }
 printf("Uredjeni brojevi: %.2f %.2f %.2f\n", x, y, z);
 return 0; 
}
int n,k;
 printf(" Unesite broj: ");
 scanf("%d",&n);
 printf(" Unesite poziciju tog broja koju zelite da proverite: ");
 scanf("%d",&n);
 if((n & (1 << k))!=0)
 printf(" Bit je 1\n");
 else
 printf(" Bit je 0\n");
 return 0;

