// septembar 2021 ispit(ispisati drugi najveci broj od n brojeva)
{
  int n, num, prvi = 0, drugi = 0;  
  
    printf("Unesi broj brojeva\n");  
    scanf("%d", &n);  
  
    printf("Unesi %d pozitivnih projeva\n", n);  
   
  
    while(n > 0)  
    {  
        scanf("%d", &num);  
  
        if(num > prvi)  
        {  
            drugi = prvi;  
            prvi = num;  
        }  
        if(num > drugi && num < prvi)  
        {  
            drugi = num;  
        }  
  
        n--;  
    }  
  
    printf("prvi najveci je %d\n",prvi);  
    printf("drugi najveci je %d\n", drugi);  
  
    return 0; 
}
// ispit jul 2021
{
int sn=0,sp=0,cifre,n,i,k=0;
printf("unesi broj");
scanf("%d",&n);
while(n>0){
    cifre=n%10;
    n/=10;
    k++;
    if(k%2==0){
        sp+=cifre;
    }
    else if(k%2!=0){sn+=cifre;}
}
if(sp!=sn){
    printf("broj nije savrsen");
}
else if(sp=sn){
    printf("broj je savrsen");
}

return 0;
}
// ispit jul 2020 
{
long sn=0,sp=0,cifre,n,i,k=0,p,proizvod,zbir,zadnjacifra,kraj;
printf("unesi broj");
scanf("%ld",&n);
p=n;
while(n>0){
    cifre=n%10;
    n/=10;
    k++;
    if(k%2==0){
        sp+=cifre;
    }
    else if(k%2!=0){sn+=cifre;}
}
proizvod=3*sn;
zbir=proizvod+sp;
for(i=zbir;i<=zbir+10;i++){
    if(i%10==0){
        kraj=i;
    }
}
zadnjacifra=kraj-zbir;
printf("ceo broj je %ld%ld",p,zadnjacifra);
return 0;
}
