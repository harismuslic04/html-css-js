{
 int sat, min, sec, n;
 printf("n= ");
 scanf("%d",&n);
 for(sat=0; sat<=23; sat++)
 for(min=0; min<=59; min++)
 for(sec=0; sec<=59; sec++)
 {
 if(n==(sat/10+sat%10+min/10+min%10+sec/10+sec%10))
 printf("\n%d sat %d min %d sec", sat, min, sec);
 }
 return 0; 
