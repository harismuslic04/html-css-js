#include <stdio.h>
#include <math.h>
int main()
{
    int x,y,n,i,brojb=0,brojc=0;
float d,p1,p2,p3,p;
printf("unesi broj tacaka n:");
scanf("%d",&n);

for(i=1;i<=n;i++){
    printf("unesi x i y:");
    scanf("%d %d",&x,&y);
    p1=fabs(1.0*(8*(6-y)+10*(y-6)+x*(6-6))/2);
p2=fabs(1.0*(10*(8-y)+9*(y-6)+x*(6-8))/2);
p3=fabs(1.0*(8*(8-y)+9*(y-6)+x*(6-8))/2);
p=fabs(1.0*(8*(6-8)+10*(8-6)+9*(6-6))/2);
d=sqrt(pow(x-9,2)+pow(y-3,2));
    if(d<=1&& d>=0){
    brojb++;
}
else if(p1+p2+p3==p){
    brojb++;
}
 else if(x>=2&&x<=10&&y>=2&&y<=4||x>=2&&x<=10&&y>=6&&y<=8||x>=2&&x<=4&&y>=2&&y<=8||x>=8&&x<=10&&y>=2&&y<=8){
    brojc++;
} else if(y<2||y>8){
    brojb++;
}else if(x<2||x>10){
    brojb++;
}
}
printf(" broj belih je %d:\t",brojb);
printf(" broj crvenih je %d:",brojc);
return 0;

#include <stdio.h>
#include <math.h>
int main()
{
    int i,n,a,b,c,d,broj,suma,suma1=0,S;
    printf("unesi sumu");
    scanf("%d",&suma);
    while(1){
        printf("unesi broj");
        scanf("%d",&broj);
        
        a=broj%10;
        b=(broj/10)%10;
        c=(broj/100)%10;
        d=(broj/1000)%10;
        
        for(i=2;i<=3;i++){
           if(a%i==0 ||b%i==0 ||c%i==0 ||d%i==0)
           broj=0;
        }
        suma1=suma1+broj;
         if(suma1>suma) {
            break;
        }
               
        }
    
    printf ("suma = %d" ,suma1);
    return 0;
}
//zadatak sa trouglom
{
int x,y;
float p1,p2,p3,p;
printf("unesi x i y tacke A");
scanf("%d %d",&x,&y);
p1=fabs(1.0*(-1*(1-y)+3*(y-1)+x*(1-1))/2);
p2=fabs(1.0*(3*(3-y)+2*(y-1)+x*(1-3))/2);
p3=fabs(1.0*(2*(1-y)-1*(y-3)+x*(3-1))/2);
p=fabs(1.0*(-1*(1-3)+3*(3-1)+2*(1-1))/2);
if(p==p1+p2+p3){
    printf("tacka se nalazi u trouglu");
}
else{
    printf("tacka se ne nalazi u trouglu");
}
return 0;
}
//krug
#include <math.h>
int main()
{
int x,y,x1,y1;
float t,r;
printf("unesi x i y tacke A");
scanf("%d %d",&x,&y);
printf("unesi poluprecnik kruga");
scanf("%f",&r);
x1=3;
y1=3;
t=pow(x-x1,2)+(y-y1,2);
if(t<=r*r){ printf("u krugu je");}
else {
    printf("nije u krugu");
}
  return 0;  
}

