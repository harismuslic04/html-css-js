for(i=1;i<=n;i++){
    printf("unesi kordinate %d . tacke",i);
    scanf("%d%d",&a,&b);
    if(a>0 && b>0){
        k1++;
    }
    else if(a<0 && b>0){
        k2++;
    }
else if(a<0 && b<0){
    k3++;
}    
else if(a>0 && b<0){
    k4++;
}
}
