int main()
{
int n,i,fakt,znak=1;
float s,q;
s=0;
fakt=1;
printf("n=");
scanf("%d",&n);
for(i=1;i<=n;i++){
    fakt*=i;
    q+=i;
    s+=znak*q/fakt;
    znak=-znak;
}
printf("suma je%f",s);
return 0;
}

#include <stdio.h>
#include <math.h>
    int main()
    {
  int i, n;
 float s, p, q;
 printf("n= ");
 scanf("%d",&n);
 s=1;
 p=0;
 q=0;
 for(i=1; i<=n; i++)
 {
 p=p+cos(i);
 q=q+sin(i);
 s=s*p/(float)q;
 }
 printf("\ns= %.5f\n", s);
 return 0;
    }
