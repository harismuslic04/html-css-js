int main()
{
int n,i,fakt,znak=1;
float s,q;
s=0;
fakt=1;
printf("n=");
scanf("%d",&n);
for(i=1;i<=n;i++){
    fakt*=i;
    q+=i;
    s+=znak*q/fakt;
    znak=-znak;
}
printf("suma je%f",s);
return 0;
}

