    int main()
    {
  int i, n;
 float s, p, q;
 printf("n= ");
 scanf("%d",&n);
 s=1;
 p=0;
 q=0;
 for(i=1; i<=n; i++)
 {
 p=p+cos(i);
 q=q+sin(i);
 s=s*p/(float)q;
 }
 printf("\ns= %.5f\n", s);
 return 0;
    }
