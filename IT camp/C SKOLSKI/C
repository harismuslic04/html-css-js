  int a=6, b=4;
 switch (a%b)
 {
 case 0:
 a++;
 b++;
 case 1:
 a++;
 b++;
 break;
 case 2:
 a--;
 b--;
 case 3:
 a--;
 b++;
 break;
 }
 printf (" a=%d, b=%d", a, b);
 getche();
 return 0; 
 {
 float op1,op2;
 char op;
 printf(" Operator: ");
 scanf ("%c", &op);
 printf(" Operand1: ");
 scanf ("%f", &op1);
 printf(" Operand2: ");
 scanf ("%f", &op2);
 switch(op)
 {
 case '+':
 printf("Vrednost: %.2f\n", op1 + op2);
 break;
 case '-':
 printf("Vrednost: %.2f\n", op1 - op2);
 break;
 case '*':
 printf("vrednost: %.2f\n", op1 * op2);
 break;
 case '/':
 if (op2 == 0)
 printf("Greska! Deljenje nulom!\n");
 else
 printf("\n Rezultat: %.2f\n", op1 / op2);
 break;
 default:
 printf(" Greska! Pogresan operator\n");
 }
 return 0;  
}
