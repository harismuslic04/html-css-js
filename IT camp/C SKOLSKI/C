 printf("n= ");
 scanf("%d",&n);
 for(i=1;i<=n;i++)
 faktorijel*=i;
 printf("Faktorijel = %ld\n", faktorijel);
 return 0; 
} ars=s/n;
 printf("\n Suma zadatih brojeva je %.2f", s);
 printf("\n Aritmeticka sredina zadatih brojeva je %.2f", ars);
 getche();
 return 0; 

