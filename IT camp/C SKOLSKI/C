// februar 2020 ispit
{
int n,m,i,k=0,p=0,pretposlednja,suma=0,z,cifre,l=0;
printf("unesi M :\n");
scanf("%d",&m);
printf("unesi N :\n");
scanf("%d",&n);
while(1){
    printf("\nunesi broj");
    scanf("%d",&i);
    pretposlednja=(i/10)%10;
    while(i>0){
        cifre=i%10;
        suma+=cifre;
        i/=10;
        k++;
    }
   
    if(k==n && suma%pretposlednja==0){
        printf("uneti broj odgovara kriterijumu");
        p++;
    }
    else{
        printf("broj ne odgovara kriterijumu");
        l++;
    }
     k=0;
    if(p==m){
        printf("\nbrojeva koji ne odgovaraju kriterijumu ima %d",l);
        break;
    }
}
return 0;
}
